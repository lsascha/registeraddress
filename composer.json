{
    "name": "afm/registeraddress",
    "description": "Register to tt_address package",
    "type": "typo3-cms-extension",
    "version": "2.0.1",
    "support": {
        "issues": "https://github.com/lsascha/registeraddress/issues"
    },
    "require": {
        "typo3/cms-core": "^10.4",
		"php": "^7.4",
		"friendsoftypo3/tt-address": "^5.2 || ^6.1",
		"undkonsorten/typo3-extbase-honeypot": "^0.1.0"
	},
    "replace": {
        "registeraddress": "self.version",
        "typo3-ter/registeraddress": "self.version"
    },
    "autoload": {
        "psr-4": {
            "AFM\\Registeraddress\\": "Classes/"
        }
    },
	"extra": {
		"typo3/cms": {
			"extension-key": "registeraddress"
		}
	}
}
